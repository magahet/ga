          "id": "j8cuyii9nne619", 
          "anon": "no", 
          "bucket_name": "This week", 
          "config": {}, 
          "bucket_order": 4, 
          "data": {
            "embed_links": null
          }, 
          "children": [], 
          "subject": "it&#39;s $$((a_i  x &#43; b_i)\\!\\!\\! \\mod P)\\!\\!\\! \\mod n$$"
        }, 
        {
          "folders": [], 
          "updated": "2017-10-04T15:21:09Z", 
